{
  "name": "hilfling-photo-backend",
  "version": "1.0.0",
  "description": "Backend service for hilfling",
  "main": "index.js",
  "scripts": {
    "start": "nodaemon --watch server --ext ts --exec ts-node --ignore '*.test.ts' --delay 3 src/server.ts",
    "test": "jest --forceExit --coverage --verbose",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix"
  },
  "keywords": [
    "nodejs",
    "express",
    "material-ui"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Fotogjengen/hilfling-photo-backend.git"
  },
  "author": "Pernille Klevstuen (peklevstuen@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Fotogjengen/hilfling-photo-backend/issues"
  },
  "homepage": "https://github.com/Fotogjengen/hilfling-photo-backend#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "nodaemon": "0.0.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.1",
    "@types/node": "^12.7.5",
    "@types/body-parser": "^1.17.1",
    "@types/errorhandler": "0.0.32",
    "eslint": "^6.3.0",
    "jest": "^24.9.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.6.3"
  }
